<workshop>
    <title>
        <!-- ID used (in conjunction with the year) to name some of the generated files -->
        <id>LDOW-</id>
        <!-- acronym of the event -->
        <acronym>LDOW</acronym>
        <!-- long name of the event -->
        <volume>Linked Data on the Web</volume>
        <!-- full title of the proceedings volume -->
        <full>Workshop on Linked Data on the Web</full>
    </title>

    <!-- CEUR-WS.org proceedings volume number

         Take care: You usually don't know this number in advance, at least not in the case that there are multiple submissions to CEUR-WS.org on the same day.  We recommend that you ask in advance for a volume number, as explained in the “Procedure PUT” section of http://ceur-ws.org/HOWTOSUBMIT.html.
         -->
    <number>2073</number>

    <!-- homepage of the event -->
    <homepage>http://events.linkeddata.org/ldow2018/</homepage>

    <!-- main language of the proceedings (ISO 639-2/T language code, e.g. eng, deu, fra, spa, rus, ita, por, ..., or look up at https://en.wikipedia.org/wiki/List_of_ISO_639-2_codes -->
    <language>eng</language>

    <!-- location and date of the event.  Note: if you create a BibTeX database to make it easier for others to cite your papers, this information will also be included.  If a LaTeX document author uses  BibLaTeX (http://ctan.org/pkg/biblatex), this information will be used. -->

    <!-- location; please also look it up on the English Wikipedia (http://en.wikipedia.org) and copy that URL into href="..." -->
    <location href="http://en.wikipedia.org/wiki/Lyon">Lyon, Paris</location>
    <!-- date of a one-day event in YYYY-MM-DD format -->
    <date>2018-04-23</date>
    <!-- alternatively: date of a multi-day event -->
    <!-- <date> -->
    <!--     <from>2013-06-18</from> -->
    <!--     <to>2013-06-20</to> -->
    <!-- </date> -->
    <!-- year in which the final papers of the proceedings were produced; uncomment and edit if different from the year in <date> -->
    <!--
    <pubyear>2013</pubyear>
    -->

    <!-- conference this workshop is co-located with;
         remove the whole <conference>...</conference> element if the workshop is standalone -->
    <conference>
        <acronym>TheWebConf 2018</acronym>
        <full>The Web Conference 2018</full>
        <homepage>https://www2018.thewebconf.org/</homepage>
    </conference>
